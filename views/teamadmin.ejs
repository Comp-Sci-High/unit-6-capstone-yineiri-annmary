<!DOCTYPE html>
<html>
<head>
    <title>Dance Team Members</title>
    <link rel="stylesheet" href="/style.css" />
          <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.6/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-4Q6Gf2aSP4eDXB8Miphtr37CMZZQ5oXLH2yaXMJ2w8e2ZtHTl7GptT4jmndRuHDT" crossorigin="anonymous">

</head>
<body> 

    <nav class="navbar navbar-expand-lg bg-body-tertiary">
  <div class="container-fluid">
    <a class="navbar-brand" href="">Fearless Vision</a>
    <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNavDropdown" aria-controls="navbarNavDropdown" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNavDropdown">
      <ul class="navbar-nav">
        <li class="nav-item">
          <a class="nav-link active" aria-current="page" href="home.html">Home</a>
        </li>
        <li class="nav-item">
          <a class="nav-link" href="/team">Team Members</a>
        </li>
        <li class="nav-item">
          <a class="nav-link" href="/event">Events</a>
        </li>
      </ul>
    </div>
  </div>
</nav>


    <h1>Our Dance Team</h1>
    <div class="team-container">
      <% for (let i=0; i < students.length; i++){ %>
        <div class="card">
      <p>Name: <%= students[i].name %></p>
      <img src="<%= students[i].image %>">
      <p>Grade: <%= students[i].grades %></p>
      <p>About: <%= students[i].description %></p>
       <form onsubmit="updateStudent(event, '<%= students[i]._id%>')">
        <label>Name:</label>
            <input type="string" name="name">
            <label>Description:</label>
            <input type="string" name="description">
          <label>Grades:</label>
            <input type="string" name="grades">
            <label>image:</label>
            <input type="string" name="image">

            <button type="submit">Submit</button>
           
          </form>
      
      
      <button onclick="deleteStudent('<%= students[i]._id %>')">Delete</button>
      </div>

       <%  } %>
    </div>


 <section id="contact" class="contact-section">
      <h2>Contact Us</h2>
      <p>IG: @csh_dance</p>
    </section>
  </main>

            <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.6/dist/js/bootstrap.bundle.min.js" integrity="sha384-j1CDi7MgGQ12Z7Qab0qlWQ/Qqz24Gc6BM0thvEMVjHnfYGF0rmFCozFSxQBxwHKO" crossorigin="anonymous"></script>
            <script src="/delete.js"></script>
 
</body>
</html>